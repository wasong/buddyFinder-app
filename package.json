    "react-modal": "1.7.7",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.1",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-thunk": "2.2.0"
