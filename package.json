  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "test": true,
      "expect": true,
      "jest": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "react/jsx-filename-extension": "off",
      "max-len": "off"
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack.config.js"
        }
      }
    }
  }
