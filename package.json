    "eslint-plugin-react": "7.0.1",
    "file-loader": "0.11.1",
    "front-matter": "2.1.2",
    "jest": "20.0.1",
    "json-loader": "0.5.4",
    "path-to-regexp": "1.7.0",
    "pixrem": "3.0.2",
    "pleeease-filters": "4.0.0",
    "postcss": "6.0.1",
    "postcss-calc": "6.0.0",
    "postcss-color-function": "4.0.0",
    "postcss-custom-media": "6.0.0",
    "postcss-custom-properties": "6.0.1",
